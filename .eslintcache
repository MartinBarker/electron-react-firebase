) initialState.rating = 3;\n  if (initialState.releaseYear === undefined) initialState.releaseYear = 2020;\n\n  const [title, setTitle] = useState(initialState.title);\n  const [rating, setRating] = useState(initialState.rating);\n  const [releaseYear, setReleaseYear] = useState(initialState.releaseYear);\n  const [errorMessage, setErrorMessage] = useState(\"\");\n\n  const onTitleChange = (event) => {\n    setTitle(event.target.value);\n  };\n  const onRatingChange = (event) => {\n    setRating(event.target.value);\n  };\n  const onYearReleasedChange = (event) => {\n    setReleaseYear(event.target.value);\n  };\n\n  const onMovieSumbit = async (event) => {\n    event.preventDefault();\n    onSubmit(title, rating, releaseYear);\n  };\n\n  return (\n    <form className=\"movie-form\" onSubmit={onMovieSumbit}>\n      <h2 className=\"movie-form__title\">Movie Details</h2>\n      {message && <p className=\"movie-form__message\">{message}</p>}\n      {errorMessage && <ErrorMessage>{errorMessage}</ErrorMessage>}\n      <fieldset className=\"movie-form__controls\" disabled={isSaving}>\n        <label className=\"movie-form__label\">Movie Title:</label>\n        <input className=\"movie-form__input\" type=\"text\" value={title} onChange={onTitleChange} />\n        <label className=\"movie-form__label\">Rating:</label>\n        <input\n          className=\"movie-form__input\"\n          type=\"number\"\n          value={rating}\n          onChange={onRatingChange}\n        />\n        <label className=\"movie-form__label\">Year Released:</label>\n        <input\n          className=\"movie-form__input\"\n          type=\"number\"\n          value={releaseYear}\n          onChange={onYearReleasedChange}\n        />\n        <input\n          className=\"movie-form__submit\"\n          type=\"submit\"\n          value={isSaving ? \"Saving...\" : \"Save\"}\n        />\n      </fieldset>\n    </form>\n  );\n}\n\nexport default MovieForm;\n","C:\\Users\\marti\\Documents\\projects\\code-sprint-b-fa2020\\FA20-CodeSprint-Code\\Code Sprint C\\Week 04\\electron-react-firebase-demo-solution\\src\\hooks\\use-all-movies.js",["173"],"import { useState, useEffect } from \"react\";\nimport { usersCollection } from \"../data/firebase\";\n\nfunction useAllMovies(userId) {\n  const [movies, setMovies] = useState([]);\n  const [isLoading, setIsLoading] = useState(false);\n  const [errorMessage, setErrorMessage] = useState(\"\");\n\n  useEffect(() => {\n    setIsLoading(true);\n\n    const onNext = (snapshot) => {\n      setIsLoading(false);\n      const docs = snapshot.docs;\n      setMovies(docs);\n    };\n\n    const onError = (error) => {\n      setIsLoading(false);\n      setErrorMessage(\"There was a problem loading your movie ratings. Please try again.\");\n      console.error(error);\n    };\n\n    const unsubscribe = usersCollection\n      .doc(userId)\n      .collection(\"movies\")\n      .orderBy(\"rating\", \"desc\")\n      .onSnapshot(onNext, onError);\n\n    return unsubscribe;\n  }, []);\n\n  return [movies, isLoading, errorMessage];\n}\n\nexport default useAllMovies;\n",["174","175"],"C:\\Users\\marti\\Documents\\projects\\code-sprint-b-fa2020\\FA20-CodeSprint-Code\\Code Sprint C\\Week 04\\electron-react-firebase-demo-solution\\src\\hooks\\use-save-movie.js",[],"C:\\Users\\marti\\Documents\\projects\\code-sprint-b-fa2020\\FA20-CodeSprint-Code\\Code Sprint C\\Week 04\\electron-react-firebase-demo-solution\\src\\hooks\\use-movie.js",["176"],"import { useState, useEffect } from \"react\";\nimport { usersCollection } from \"../data/firebase\";\n\nfunction useMovie(userId, movieId) {\n  const [isLoading, setIsLoading] = useState(false);\n  const [errorMessage, setErrorMessage] = useState(\"\");\n  const [movieData, setMovieData] = useState(null);\n\n  useEffect(() => {\n    async function getMovie() {\n      setIsLoading(true);\n\n      try {\n        const movieSnapshot = await usersCollection\n          .doc(userId)\n          .collection(\"movies\")\n          .doc(movieId)\n          .get();\n\n        if (!movieSnapshot.exists) {\n          throw new Error(\"No such movie exists!\");\n        }\n\n        const data = movieSnapshot.data();\n        setMovieData(data);\n      } catch (error) {\n        setErrorMessage(\"Something went wrong. Please try again.\");\n        console.error(error);\n      }\n\n      setIsLoading(false);\n    }\n\n    getMovie();\n  }, [movieId]);\n\n  return [movieData, isLoading, errorMessage];\n}\n\nexport default useMovie;\n","C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\index.js",["177"],"C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\components\\app.js",[],"C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\data\\firebase.js",[],"C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\components\\nav.js",[],"C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\pages\\account-page.js",[],"C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\pages\\movies-page.js",[],"C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\pages\\edit-movie-page.js",[],"C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\pages\\not-found-page.js",[],"C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\pages\\add-movie-page.js",[],"C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\components\\account-info.js",[],"C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\components\\movie-listing.js",[],"C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\components\\edit-movie.js",[],"C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\components\\add-movie.js",[],"C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\components\\error-message.js",[],"C:\\Users\\marti\\Documents\\projects\\electron-react-firebase\\src\\components\\movie.js",[],"C:\\Users\\m